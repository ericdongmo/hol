pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sleep 5
            }
        }
         stage('build') {
            steps {
                echo 'Hello build'
                sleep 4
            }
        }
         stage('deploy') {
            steps {
                echo 'Hello deploy'
                sleep 3
            }
        }
         stage('test') {
            steps {
                echo 'Hello test'
                sleep 3
            }
        }
                
    }
}
